{
  "name": "textbook-generation-frontend",
  "version": "1.0.0",
  "description": "Frontend for AI-powered textbook generation system",
  "private": true,
  "dependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.1",
    "styled-components": "^6.0.7",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:unit": "vitest src",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.3",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.45.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "textbook",
    "ai",
    "education",
    "react",
    "physical-ai",
    "humanoid-robotics"
  ],
  "author": "AI Education Platform",
  "license": "MIT"
}
